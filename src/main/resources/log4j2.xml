<?xml version="1.0" encoding="UTF-8"?>
<configuration strict="true">
    <appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%-4level] %c{1.} - %msg%n" />
        </Console>
    </appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="Console" />
        </Root>
    </Loggers>
</configuration>
